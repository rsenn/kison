{
  "name": "vba",
  "version": "0.0.11",
  "description": "vba typescript/javascript runtime",
  "author": "yiminghe <yiminghe@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:yiminghe/kison.git"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "pika-plugin-ts-types",
        {
          "args": [
            "--rootDir",
            "src"
          ],
          "tsconfig":"tsconfig.build.json"
        }
      ],
      [
        "pika-plugin-build-web-babel",
        {
          "format": "cjs",
          "sourcemap":false
        }
      ],
      [
        "pika-plugin-build-web-babel",
        {
          "mjs": true,
          "sourcemap":false
        }
      ],
      [
        "pika-plugin-clean-dist-src"
      ]
    ]
  },
  "scripts": {
    "test": "rm -rf pkg && jest",
    "test-debug": "jest -i",
    "check":"tsc --noEmit",
    "build": "npm run check && pika-pack build && cp src/*.d.ts pkg/dist-types/",
    "gen": "sh scripts/gen.sh",
    "pub": "sh scripts/pub.sh && rm -rf pkg"
  }
}