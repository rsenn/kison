{
  "name": "kison",
  "version": "0.5.45",
  "description": "A LALR(1)/LL(1)/LL(K) parser generator for javascript/typescript",
  "author": "yiminghe <yiminghe@gmail.com>",
  "files": [
    "pkg",
    "types",
    "bin"
  ],
  "types": "types",
  "keywords": [
    "LALR",
    "LL",
    "LLK",
    "parser generator"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:yiminghe/kison.git"
  },
  "workspaces": [
    "examples/*"
  ],
  "devDependencies": {
    "@babel/cli": "7.x",
    "@babel/core": "7.x",
    "@babel/node": "7.x",
    "@babel/preset-env": "7.x",
    "@babel/preset-typescript": "7.x",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "27.x",
    "@types/node": "^17.0.8",
    "@yiminghe/rr": "1.x",
    "babel-jest": "27.x",
    "benchmark": "^2.1.4",
    "fs-extra": "10.x",
    "husky": "7.x",
    "jest": "27.x",
    "lint-staged": "^7.2.0",
    "np": "^7.x",
    "prettier": "2.x",
    "semver": "7.x",
    "supports-color": "7.x",
    "typescript": "4.x"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*"
    ],
    "testMatch": [
      "**/*.test.[j|t]s?(x)"
    ]
  },
  "dependencies": {
    "commander": "2.x",
    "debug": "4.x",
    "esbuild": "^0.14.10",
    "modulex-util": "1.x"
  },
  "scripts": {
    "prepack": "yarn run compile",
    "prepare": "husky install",
    "compile": "babel -x '.js,.ts' src --out-dir pkg && cp src/*.d.ts pkg/",
    "check": "tsc --noEmit",
    "start": "http-server --mimetypes examples/mime.types",
    "reinstall": "rm -rf node_modules .yarn/cache yarn.lock && ayarn",
    "pub": "yarn run check && rm -rf pkg && yarn run compile && yarn np --no-cleanup  --no-publish --no-release-draft && npm publish",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"{src,__tests__,examples,bin}/**/*.{js,tsx,ts,jsx}\"",
    "test": "jest",
    "test-debug": "rm -rf pkg && jest -i",
    "g:ts": "cd $PWD && babel-node -x '.js,.ts,.tsx',.mjs"
  },
  "bin": "./bin/kison.js",
  "lint-staged": {
    "*.{tsx,js,jsx,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "packageManager": "yarn@3.1.1"
}
