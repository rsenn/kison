{
  "name": "kison",
  "version": "0.4.36",
  "description": "LALR parser generator for javascript",
  "author": "yiminghe <yiminghe@gmail.com>",
  "files": [
    "lib",
    "bin"
  ],
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:yiminghe/kison.git"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/node": "7.x",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.15.0",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-standard-pkg": "^0.6.0",
    "@types/jest": "27.x",
    "@yiminghe/rr": "1.x",
    "babel-jest": "^27.0.2",
    "benchmark": "^2.1.4",
    "fs-extra": "10.x",
    "jest": "27.x",
    "lint-staged": "^7.2.0",
    "monaco-editor": "^0.28.0",
    "np": "^7.x",
    "pika-plugin-build-web-babel": "0.11.x",
    "pika-plugin-clean-dist-src": "^0.1.1",
    "pre-commit": "^1.2.2",
    "prettier": "2.x"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/*"
    ],
    "testMatch": [
      "**/*.test.[j|t]s?(x)"
    ]
  },
  "dependencies": {
    "commander": "2.x",
    "debug": "4.x",
    "modulex-util": "1.x"
  },
  "scripts": {
    "start": "http-server --mimetypes examples/mime.types",
    "reinstall": "rm -rf node_modules yarn.lock && ayarn",
    "prepare": "node scripts/link",
    "perf-formula": "BABEL_ENV=test npx babel-node examples/excel-formula-ll/benchmark/index.js",
    "ts-formula": "BABEL_ENV=test npx babel-node -x '.js,.ts' examples/excel-formula-ll/tests/testTs.ts",
    "pub": "np --no-cleanup  --no-publish --no-release-draft && npm publish",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"{lib,__tests__,examples,bin}/**/*.{js,tsx,ts,jsx}\"",
    "test": "jest",
    "test-debug": "jest -i"
  },
  "bin": {
    "kison": "./bin/kison.js"
  },
  "lint-staged": {
    "*.{tsx,js,jsx,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ]
}
